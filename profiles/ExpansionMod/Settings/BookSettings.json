    "RuleCategories": [
        {
            "CategoryName": "General",
            "Rules": [
                {
                    "RuleParagraph": "No Greifing",
                    "RuleText": "Getting your kicks at the expense of others is considered griefing. Lowering flags is considered griefing and will result in a ban."
                },
                {
                    "RuleParagraph": "No Hate Speech",
                    "RuleText": "All are welcome here. Any hate speech will result in an immediate ban."
                },
                {
                    "RuleParagraph": "Join the Discord",
                    "RuleText": "A full list of rules is posted in the Discord at http://discord.42MonthsAfter.com"
                }
            ]
        },
        {
            "CategoryName": "Base Building Rules",
            "Rules": [
                {
                    "RuleParagraph": "One Base Per Player / Group",
                    "RuleText": "Groups & individual players are allowed one (1) base at a time."
                }
            ]
        },
        {
            "CategoryName": "Base Raiding Rules",
            "Rules": [
            {
                "RuleParagraph": "Base Raiding Rules",
                "RuleText": "Base raiding is allowed 24/7 by any means possible. Secure your shit."
            }
            ]
        },
        {
            "CategoryName": "Vehicles",
            "Rules": [
                {
                    "RuleParagraph": "Us at Your Own Risk",
                    "RuleText": "DayZ vehicles are a mess, always have been, there for all are use at your own risk."
                },
                {
                    "RuleParagraph": "Admin Help",
                    "RuleText": "Admins will NOT replace lost vehicles due to lag, crashes, getting stuck, etc. See rule above."
                },
                {
                    "RuleParagraph": "Zombie Damage",
                    "RuleText": "Zombies can damage vehicles, based on chance, so don't just drive over them without being prepared to lose your car to damage."
                }
            ]
        },
        {
            "CategoryName": "PvP Rules",
            "Rules": [
                {
                    "RuleParagraph": "The whole map is PvP",
                    "RuleText": "Anything goes in PvP including kill on sight. Be warned."
                }
            ]
        }
    ],
